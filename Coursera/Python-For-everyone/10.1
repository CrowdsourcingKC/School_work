import string
name = raw_input("Enter file:")
if len(name) < 1 : name = "mbox-short.txt"
handle = open(name)

times = dict()

for line in handle:
    if not line.startswith("From "): continue
    words = line.split()
    times[words[1]]=times.get(words[1],0)+1 #This is the important part
lst = list()
for key, val in times.items():
    lst.append((val, key))

lst.sort(reverse=True)

for key,val in lst[:1]:
    print val, key
